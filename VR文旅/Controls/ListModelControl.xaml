<UserControl x:Class="VR文旅.Controls.ListModelControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:MyNamespace="clr-namespace:VR文旅.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid x:Name="grid">
        <Grid.RowDefinitions>
            <RowDefinition Height="202*"/>
            <RowDefinition Height="23*"/>
        </Grid.RowDefinitions>
        <MyNamespace:PaginationControl x:Name="pagination" PageChanged="Pagination_PageChanged" FontSize="22" Grid.Row="1"/>
        <StackPanel x:Name="panel"/>
    </Grid>
</UserControl>
